{
   "action": {
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "commands": {
      "open-incognito": {
         "description": "Open current url in incognito window",
         "suggested_key": {
            "default": "Alt+Q",
            "mac": "Alt+Q"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "description": "Tool to provide easier access to research work",
   "icons": {
      "16": "assets/sci-hub.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "90",
   "name": "Sci-Hub Multi",
   "permissions": [ "contextMenus", "tabs", "storage", "downloads", "history" ],
   "short_name": "Sci-Hub Multi",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.5"
}
